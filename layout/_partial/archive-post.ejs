    <header class="archive-article-header">
      <%- partial('post/date', {class_name: 'archive-article-date', date_format: __('article_archive_date_format')}) %>
      <%- partial('post/title', {class_name: 'archive-article-title', archive: true }) %>
    </header>
  </div>
</article>